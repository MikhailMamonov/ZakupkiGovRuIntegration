@model ZakupkiGovRuIntegration.Models.PurchaseModel
@{
    ViewData["Title"] = "Home Page";
}


@using (Html.BeginForm("GetData","Home",FormMethod.Post))
{
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th colspan="2" align="center">Введите регистрационный номер аукциона.</th>
        </tr>
        <tr>
            <td>Регистрационный номер: </td>
            <td>
                @Html.TextBoxFor(m => m.RegNumber)
            </td>
        </tr>
        <tr>
        <tr>
            <td><input type="submit" value="Загрузить" /></td>
        </tr>
    </table>
}
